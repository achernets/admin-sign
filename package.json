{
  "name": "almex-contractor-admin",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^4.0.3",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.8.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "rc-resize-observer": "^0.1.3",
    "react": "^16.13.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.13.0",
    "react-drag-listview": "^0.1.6",
    "react-redux": "^7.2.0",
    "react-redux-i18n": "^1.9.3",
    "react-resizable": "^1.10.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "react-window": "^1.8.5",
    "redux": "^4.0.5",
    "redux-async-initial-state": "^0.3.0",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "EXTEND_ESLINT=true react-app-rewired start",
    "build": "EXTEND_ESLINT=true react-app-rewired build",
    "eject": "react-scripts eject",
    "thrift": "node thrift/index.js"
  },
  "eslintConfig": {
    "extends": "react-app",
    "globals": {
      "KazFilter": true,
      "FilterFieldType": true,
      "FilterCondition": true,
      "FilterItem": true,
      "MrkUserSession": true
    },
    "rules": {
      "react-hooks/exhaustive-deps": "off",
      "no-console": "warn",
      "no-unused-vars": "warn",
      "no-unexpected-multiline": "warn",
      "quotes": [
        "warn",
        "single",
        {
          "allowTemplateLiterals": true
        }
      ],
      "semi": [
        "warn",
        "always"
      ],
      "jsx-quotes": [
        "warn",
        "prefer-double"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-plugin-import": "^1.13.0",
    "child_process": "^1.0.2",
    "concat-files": "^0.1.1",
    "customize-cra": "^0.9.1",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "node-sass": "^4.13.1",
    "react-app-rewired": "^2.1.5"
  }
}